version 18
__lua__
-- hundstein 3d
-- by schoblaska

-- [x] untextured raycasting
-- [x] wall textures
-- [x] sprites
-- [x] give treats to dogs
-- [x] method to step through demo (raycasting screen, flat and buggy, flat, textured, sprites)
-- [x] map editor
-- [x] bottom-of-screen ui
-- [x] "accent" walls
-- [x] raycasting explanation screen
-- [x] more sprites
-- [x] title screen
-- [x] sort sprites by distance to player

-- [ ] pick up treats before giving
-- [ ] music
-- [ ] when "wielding" treats, show on bottom of screen and bob when walking
-- [ ] doors (use sprites that don't rotate?)
-- [ ] only succeed to give treat if dog is in front of player
-- [ ] sound effects
-- [ ] make the screen flash when picking up treasure

#include utils/decompressor.p8
#include utils/distance.p8

#include hund3d/map.p8
#include hund3d/player.p8
#include hund3d/rays.p8
#include hund3d/sprites.p8
#include hund3d/title.p8
#include hund3d/ui.p8

function _init()
  init_sprites()
  init_player()
  init_map()
  init_decompressor()

  load_splash()

  showSplash = 90
  rotSpeed = 0.01
  moveSpeed = 0.25
  showInstructions = true
  givingTreat = false
  treatY = 128
  mode = 0
end

function _update()
  if mode == 0 then
    -- splash screen
    if showSplash <= 0 or btnp() > 0 then
      load_title()
      mode += 1
    else
      showSplash -= 1
    end
  elseif mode == 1 then
    -- title slide
    if btnp(4) or btnp(5) then
      reload(0, 0, 8192) -- reload gfx memory
      mode += 1
    end
  elseif mode == 2 then
    move()
    give_treat()
    get_treasure()
  end
end

function _draw()
  cls()

  if mode == 0 then
    draw_splash()
  elseif mode == 1 then
    draw_title()
  elseif mode == 2 then
    draw_rays()
    draw_treat()
    draw_ui()
    if showInstructions then draw_instructions() end
  end
end

function draw_instructions()
  print("hold z to strafe", 1, 1, 6)
  print("hold x to give treat", 1, 8, 6)
end

